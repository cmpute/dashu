from typing import Union, overload

class Words:
    pass

class UBig:
    def __new__(obj: Union[int, str]): ...
    def to_words(self) -> Words: ...
    @staticmethod
    def from_words(self, words: Words) -> UBig: ...

    @overload
    def __add__(self, other: UBig) -> UBig: ...
    @overload
    def __add__(self, other: Union[int, IBig]) -> IBig: ...

class IBig:
    def __new__(obj: Union[int, str]): ...
